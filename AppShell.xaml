<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FlyoutTabNavPrefi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FlyoutTabNavPrefi.Pages"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
       <ShellContent
            Title="Taylor Swift"
            ContentTemplate="{DataTemplate local:TaylorSwiftPage}"
            Icon="taylor.jpg"
            Route="TaylorSwiftPage" />

        <ShellContent
             Title="Niki"
             ContentTemplate="{DataTemplate local:NikiPage}"
             Icon="niki.png"
             Route="NikiPage" />

        <ShellContent
             Title="Sabrina Carpenter"
             ContentTemplate="{DataTemplate local:SabrinaCarpenterPage}"
             Icon="sabrina.jpg"
             Route="SabrinaCarpenterPage" />
    </FlyoutItem>
    

</Shell>
